{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "jquery@3.6.0",
            "destination": "wwwroot/lib/jquery/"
        },
        {
            "library": "twitter-bootstrap@5.1.1",
            "destination": "wwwroot/lib/bootstrap/",
            "files": [
              "js/*.js",
              "js/*.js.map"
            ]
        },
        {
            "library": "jquery-validate@1.19.2",
            "destination": "wwwroot/lib/jquery-validate/",
          "files": [
            "*.js",
            "*.js.map"
          ]
        },
        {
            "library": "jquery-validation-unobtrusive@3.2.12",
            "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
        },
        {
            "library": "moment.js@2.29.1",
            "destination": "wwwroot/lib/moment.js/"
        },
        {
            "library": "vue@2.6.14",
            "destination": "wwwroot/lib/vue/"
        },
        {
            "library": "vuetify@2.6.2",
            "destination": "wwwroot/lib/vuetify/",
            "files": [
                "*.js",
                "*.js.map",
                "json/*.json"
            ] 
        }
    ]
}