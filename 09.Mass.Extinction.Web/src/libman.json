{
    "version": "1.0",
    "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "scss/bootstrap/",
      "files": [
        "scss/**/*.scss"
      ]
    },
    {
      "library": "vuetify@2.6.2",
      "destination": "scss/vuetify/",
      "files": [
        "**/*.css",
        "**/*.css.map"
      ]
    }
    ]
}